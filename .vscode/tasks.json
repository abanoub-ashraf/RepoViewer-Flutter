{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build runner",
            "type": "shell",
            "command": "fvm flutter packages pub run build_runner build --delete-conflicting-outputs",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "watch runner",
            "type": "shell",
            "command": "fvm flutter packages pub run build_runner watch --delete-conflicting-outputs",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "pub get",
            "type": "shell",
            "command": "fvm flutter pub get",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
    ]
}